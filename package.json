{
  "name": "trello",
  "version": "1.0.0",
  "main": "server/server.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "eslint .",
    "prestart": "npm run --prefix client build",
    "start": "node .",
    "start-dev": "concurrently \"node .\" \"npm start --prefix client\"",
    "postinstall": "npm install --prefix client",
    "posttest": "npm run lint && nsp check",
    "prettier:all": "prettier-eslint --write \"$PWD/client/src/**/*.{js,jsx}\"",
    "prettier:file": "prettier-eslint --write"
  },
  "dependencies": {
    "compression": "^1.0.3",
    "concurrently": "^5.1.0",
    "cors": "^2.5.2",
    "helmet": "^3.10.0",
    "loopback": "^3.19.0",
    "loopback-boot": "^2.6.5",
    "loopback-component-explorer": "^6.0.0",
    "serve-favicon": "^2.0.1",
    "strong-error-handler": "^2.0.0"
  },
  "private": true,
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@mean-expert/loopback-sdk-builder": "^2.3.1",
    "babel-eslint": "^10.1.0",
    "eslint": ">=4.18.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-loopback": "^13.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "https-proxy-agent": "5.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.3",
    "lodash": "4.17.19",
    "nsp": "3.2.1",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "swagger-ui": ">=3.23.11"
  },
  "repository": {
    "type": "",
    "url": ""
  },
  "license": "UNLICENSED",
  "description": "trello"
}
